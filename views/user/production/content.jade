.carousel_container
	.content.breadcrumbs_content
		include ../helpers/breadcrumbs
	
	#production_carousel.carousel.slide(data-ride="carousel")
		.carousel-inner(role="listbox")
			.item.active
				.banner_image.banner_image_1
				.content
					.top_label
						div «Завод Агуша» –
						div питание только
						div для деток
					
					.cloud_text.adorable_table
						.adorable_cell
							div На «Заводе Агуша» детское питание изготавливают по уникальной рецептуре исключительно
							div для малышей
		
		//- .content.indicators_content
			ol.carousel-indicators
				li.active(data-target="#production_carousel" data-slide-to="0")

.tabs
	.content
		.tabs_flex
			.tab.tab_selector.advantages(data-tab="advantages")
				.tab_selector_inside
					.title Преимущества
			
			.tab.tab_selector.production.active(data-tab="production")
				.tab_selector_inside
					.title Как производят агушу

//- .white_title.arrowed
	.content
		.label.adorable_table
			.adorable_cell Как производят агушу

.tab_contents
	.tab_content.tab_selector.advantages
		.advantages_container
			.top_label
				div Вот почему «Агуша» – лучшее
				div для малыша:
			
			.advantages_info
				.content
					.advantage.advantage_1
						.advantage_number 1
						
						.advantage_title
							div Молоко
							div экстра-класса
						
						.advantage_desc
							div Продукция изготавливается
							div из натурального молока
							div высшего качества
					
					.advantage.advantage_2
						.advantage_number 2
						
						.advantage_title
							div Полезные
							div бактерии
						
						.advantage_desc
							div Не содержит красителей,
							div консервантов и стабилизаторов
					
					.advantage.advantage_3
						.advantage_number 3
						
						.advantage_title Оборудование
						
						.advantage_desc
							div Современное оборудование
							div позволяет сохранить ценные
							div аминокислоты
					
					.advantage.advantage_4
						.advantage_number 4
						
						.advantage_title
							div Ягоды
							div и фрукты
						
						.advantage_desc
							div Фруктовые наполнители
							div производятся специально для
							div детского питания
					
					a.new_module(href = "/products")
						button.to_catalog В каталог
	
